<AnchorPane prefHeight="125.0" prefWidth="100.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="controller.PhotoViewController$PhotoPaneController">
	<children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="100.0">
         <children>
      		<ImageView fx:id="imageview" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </HBox>
		<Label fx:id="labelCaption" alignment="CENTER" layoutX="15.0" layoutY="100.0" prefHeight="6.0" prefWidth="70.0" text="OMG OMG" textAlignment="CENTER">
		   <font>
			  <Font name="Nirmala UI" size="12.0" />
		   </font>
		</Label>
	</children>
</AnchorPane>
